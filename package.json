{
  "name": "n8n-nodes-better-ai-agent-return-input",
  "version": "1.7.1",
  "description": "A better AI Agent node for n8n with improved memory management and modern AI SDK integration. This will return the full input data along with the agent's response.",
  "main": "dist/BetterAiAgent.node.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "tsc --watch",
    "package": "npm run build && npm pack",
    "deploy-local": "npm run build && cp package.json ~/.n8n/custom/ && cp -R dist/* ~/.n8n/custom/ && npm install --omit=dev --prefix ~/.n8n/custom"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/BetterAiAgent.node.js",
      "dist/BetterAiAgentTool.node.js"
    ]
  },
  "keywords": [
    "n8n-nodes-base",
    "n8n",
    "n8n-community-node-package",
    "ai",
    "agent",
    "langchain",
    "openai",
    "anthropic"
  ],
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.2",
    "@ai-sdk/google": "^1.0.2",
    "@ai-sdk/openai": "^1.0.5",
    "@langchain/core": "0.1.18",
    "@opentelemetry/auto-instrumentations-node": "^0.60.1",
    "@opentelemetry/sdk-node": "^0.202.0",
    "ai": "^4.0.8",
    "langfuse-vercel": "^3.37.4",
    "langsmith": "^0.3.31",
    "n8n-workflow": ">=1.94.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.94.0"
  },
  "engines": {
    "node": ">=18",
    "n8n": ">=1.24.0"
  }
}
